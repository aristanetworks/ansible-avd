{# eos - VLANs #}
{% if vlans is defined and vlans is not none %}
{%     for vlan in vlans | natural_sort %}
vlan {{ vlan }}
   name {{ vlans[vlan].name }}
{%         if vlans[vlan].trunk_groups is defined %}
{%             for trunk_group in vlans[vlan].trunk_groups | natural_sort %}
   trunk group {{ trunk_group }}
{%             endfor %}
{%         endif %}
!
{%     endfor %}
{% endif %}